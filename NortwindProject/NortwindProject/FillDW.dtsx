      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{3D1505AB-7C32-40F5-ABFE-4B46568978BA}"
          SQLTask:SqlStatementSource="INSERT INTO FactSales(OrderLine, ProductKey, CustomerKey, &#xA;OrderDateKey, OrderUnitPrice, OrderQuantity, OrderDiscount)&#xA;SELECT od.OrderID, od.productID, dc.CustomerKey, CAST(FORMAT(o.OrderDate,'yyyyMMdd') as int), od.UnitPrice, od.Quantity, od.Discount&#xA;from Northwind.dbo.OrderDetails od JOIN Northwind.dbo.Orders o ON od.OrderID = o.OrderID&#xA;join DimCustomer dc on o.CustomerID = dc.CustomerID&#xA;WHERE &#xA;/* Slowly Changing Dimension dimCustomer */&#xA;o.OrderDate &gt;= dc.start and (dc.[end] is null or o.orderdate &lt;= dc.[end]) &#xA;AND&#xA;/* only add new lines + make sure it runs from an empty FactSales table */&#xA;o.OrderID &gt; (SELECT ISNULL(MAX(OrderLine),0) from FactSales)&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
